@model string;

@{
    Layout = Model;
}
<!-- Content -->
<div class="middle-box text-center animated fadeIn">
    <h1>10-4</h1>
    <h3 class="font-bold">Success!</h3>

    <div class="error-desc">
        We got it!  Your password has been changed !
        <br />
        You may now continue with your work.<br />
        <br />
        <a id="homeLink" href="javascript:void(0);" class="btn btn-primary m-t">Home</a>
    </div>
</div>

@section Scripts {
    <environment include="Development">
        <script src="~/js/common/Password/success.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/common/Password/success.min.js" asp-append-version="true"></script>
    </environment>
}